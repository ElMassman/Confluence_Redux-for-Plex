<includes>
  <include name="NowPlayingCurrentCover">
    <control type="group">
      <description>Current cover</description>
      <visible>Player.HasMusicPlaylist</visible>
      <control type="image">
        <posy>332</posy>
        <width>330</width>
        <height>330</height>
        <info>MusicPlayer.Cover</info>
        <aspectratio>stretch</aspectratio>
        <texture flipy="true" diffuse="NowPlayingReflectionDiffuse.png"/>
      </control>
      <control type="image">
        <posx>-2</posx>
        <posy>-1</posy>
        <width>341</width>
        <height>341</height>
        <aspectratio>stretch</aspectratio>
        <texture>NowPlayingCoverDropShadow.png</texture>
      </control>
      <control type="image">
        <width>330</width>
        <height>330</height>
        <info>MusicPlayer.Cover</info>
        <aspectratio>stretch</aspectratio>
        <texture diffuse="NowPlayingCoverDiffuse.png"/>
      </control>
      <control type="image">
        <width>330</width>
        <height>330</height>
        <aspectratio>stretch</aspectratio>
        <texture>NowPlayingCoverOverlay.png</texture>
      </control>
    </control>
  </include>
  
  <include name="NowPlayingNextCover">
    <control type="group">
      <description>Next cover</description>
      <visible>Player.HasMusicPlaylist</visible>
      <control type="image">
        <width>264</width>
        <height>264</height>
        <info>MusicPlayer.NextNewCover</info>
        <aspectratio>stretch</aspectratio>
        <texture diffuse="NowPlayingSmallCoverDiffuse.png"/>
      </control>
      <control type="image">
        <posy>264</posy>
        <width>264</width>
        <height>264</height>
        <info>MusicPlayer.NextNewCover</info>
        <aspectratio>stretch</aspectratio>
        <texture flipy="true" diffuse="NowPlayingSmallReflectionDiffuse.png"/>
      </control>
      <control type="image">
        <posx>104</posx>
        <width>176</width>
        <height>264</height>
        <aspectratio>stretch</aspectratio>
        <texture>NowPlayingCoverShadow.png</texture>
        <visible>MusicPlayer.HasNext</visible>
      </control>
      <control type="image">
        <width>264</width>
        <height>264</height>
        <aspectratio>stretch</aspectratio>
        <visible>MusicPlayer.HasNext</visible>
        <texture diffuse="NowPlayingSmallCoverDiffuse.png">NowPlayingCoverOverlay.png</texture>
      </control>
    </control>
  </include>
  
  <include name="NowPlayingCurrentInfo">
    <control type="group">
      <description>Current track info</description>
      
      <control type="label">
        <posx>500</posx>
        <width>500</width>
        <font>NPArtistFont</font>
        <textcolor>blue</textcolor>
        <label>$INFO[MusicPlayer.Artist]</label>
        <align>right</align>
      </control>
  
      <control type="label">
        <posx>500</posx>
        <posy>35</posy>
        <width>500</width>
        <font>NPTitleFont</font>
        <textcolor>grey2</textcolor>
        <label>$INFO[MusicPlayer.Album]</label>
        <align>right</align>
      </control>
  
      <control type="label">
        <posx>500</posx>
        <posy>108</posy>
        <width>500</width>
        <font>NPAlbumFont</font>
        <textcolor>white</textcolor>
        <label>$INFO[MusicPlayer.Title]</label>
        <align>right</align>
      </control>
    </control>
  </include>

